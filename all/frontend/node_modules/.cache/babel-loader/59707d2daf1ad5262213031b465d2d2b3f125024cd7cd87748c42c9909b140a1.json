{"ast":null,"code":"import Header from \"@/components/common/Header.vue\";\nexport default {\n  name: \"question_public_search\",\n  data: function () {\n    return {\n      comment_List: null,\n      s_searchdto: null,\n      s_page: null,\n      s_dto_list: null,\n      user: this.$session.get('user')\n    };\n  },\n  components: {\n    Header\n  },\n  mounted() {\n    this.$axios.get('/service/question-public-search').then(res => {\n      this.comment_List = res.comment_List;\n      this.s_searchdto = res.s_searchdto;\n      this.s_page = res.s_page;\n      this.s_dto_list = res.s_dto_list;\n      // 페이지 값\n      for (var i = 1; i < this.s_page.page_EndBno - this.s_page.page_StartBno + 1; i++) {\n        if (i == this.s_page.page_NowBno) {\n          this.$refs.e_paging_num.innerHTML += '<a id=\"page_NowBno\">' + i + '</a>';\n        } else {\n          this.$refs.e_paging_num.innerHTML += '<a ' + 'href=\"' + \"/service/question-public-search?page_NowBno=\" + i +\n          // eslint-disable-next-line\n          '&standard=' + s_page.standard + '\"' + 'class=\"page_Bno\" id=\"page_Bno' + i + '\">' + i + '</a>';\n        }\n      }\n      var standard = this.s_page.standard;\n      var search_time = this.s_searchdto.search_time;\n      var search_type = this.s_searchdto.search_type;\n      document.getElementById(search_time).setAttribute('selected', 'selected');\n      document.getElementById(search_type).setAttribute('selected', 'selected');\n      document.getElementById(standard).setAttribute('selected', 'selected');\n    });\n  },\n  methods: {\n    // 글 상세 페이지\n    blist_title_click: function (e) {\n      var val = e.target.parentElement.parentElement.firstElementChild.value;\n      this.$refs.e_bno_val.value = val;\n      var url = '/service/question-public-detail?bno=' + val;\n      location.href = url;\n    },\n    // 검색 결과 - 엔터\n    s_content_input_keydown: function (e) {\n      if (e.keyCode == 13) {\n        e.preventDefault();\n        this.$refs.s_content_btn.click();\n      }\n    },\n    // 검색 결과 전달\n    s_content_btn_click: function () {\n      // 검색 기간 등 정보\n      let e_search_time = this.$refs.e_search_time_sel.value;\n      let e_search_type = this.$refs.e_search_type_sel.value;\n      let e_search_content = this.$refs.s_content_input.value;\n      let url = \"/service/question-public-search\";\n      url += \"?search_time=\" + e_search_time;\n      url += \"&search_type=\" + e_search_type;\n      url += \"&search_content=\" + e_search_content;\n      location.href = url;\n    },\n    e_con_choice_click: function () {\n      var origin_type = this.s_page.standard;\n      var result_type = this.$refs.e_con_choice.options[this.$refs.e_con_choice.selectedIndex].value;\n      // 값이 다르면 실행\n      if (origin_type != result_type) {\n        var url = \"/service/question-public?\";\n        url += \"standard=\" + result_type;\n        location.href = url;\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAwLA,OAAOA,MAAK,MAAO,gCAAgC;AACnD,eAAe;EACbC,IAAI,EAAE,wBAAwB;EAC9BC,IAAG,EAAI,YAAW;IAChB,OAAO;MACHC,YAAW,EAAI,IAAI;MACnBC,WAAU,EAAI,IAAI;MACxBC,MAAK,EAAI,IAAI;MACbC,UAAS,EAAI,IAAI;MACjBC,IAAG,EAAI,IAAI,CAACC,QAAQ,CAACC,GAAG,CAAC,MAAM;IAC7B;EACF,CAAC;EACDC,UAAU,EAAE;IACbV;EACC,CAAC;EACDW,OAAO,GAAG;IACX,IAAI,CAACC,MAAM,CAACH,GAAG,CAAC,iCAAiC,EAC/CI,IAAI,CAAEC,GAAG,IAAG;MACZ,IAAI,CAACX,YAAW,GAAIW,GAAG,CAACX,YAAY;MACpC,IAAI,CAACC,WAAU,GAAIU,GAAG,CAACV,WAAW;MAClC,IAAI,CAACC,MAAK,GAAIS,GAAG,CAACT,MAAM;MACxB,IAAI,CAACC,UAAS,GAAIQ,GAAG,CAACR,UAAU;MAChC;MACA,KAAI,IAAIS,CAAC,GAAC,CAAC,EAAEA,CAAC,GAAC,IAAI,CAACV,MAAM,CAACW,WAAW,GAAC,IAAI,CAACX,MAAM,CAACY,aAAa,GAAC,CAAC,EAAEF,CAAC,EAAE,EAAC;QACvE,IAAIA,CAAC,IAAE,IAAI,CAACV,MAAM,CAACa,WAAW,EAAC;UAC9B,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,SAAQ,IAAK,sBAAsB,GAACN,CAAC,GAAC,MAAM;QACrE,OAAO;UACN,IAAI,CAACI,KAAK,CAACC,YAAY,CAACC,SAAQ,IAAK,KAAK,GAC1C,QAAQ,GAAC,8CAA8C,GAACN,CAAC;UACzD;UACA,YAAY,GAACV,MAAM,CAACiB,QAAQ,GAAC,GAAE,GAC9B,+BAA+B,GAACP,CAAC,GAAC,IAAI,GAACA,CAAC,GAAC,MAA