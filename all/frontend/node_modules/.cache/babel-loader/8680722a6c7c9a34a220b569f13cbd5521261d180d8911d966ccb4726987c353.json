{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport Header from \"@/components/common/Header.vue\";\nexport default {\n  name: \"question_member\",\n  components: {\n    Header\n  },\n  data() {\n    return {\n      s_page: null,\n      s_dto_list: null,\n      s_searchdto: null,\n      search_content_: null,\n      user: JSON.parse(sessionStorage.getItem('user')),\n      page_list: []\n    };\n  },\n  created() {\n    this.getaxios();\n  },\n  methods: {\n    getaxios() {\n      this.$axios.get('/service/question-member').then(res => {\n        this.s_page = JSON.parse(JSON.stringify(res.data.s_page));\n        this.s_dto_list = JSON.parse(JSON.stringify(res.data.s_dto_list));\n        this.s_searchdto = JSON.parse(JSON.stringify(res.data.s_searchdto));\n        for (var i = this.s_page.page_StartBno; i <= this.s_page.page_EndBno; i++) {\n          this.page_list.push(i);\n        }\n        var search_time = this.s_searchdto.search_time;\n        var search_type = this.s_searchdto.search_type;\n        document.getElementById(search_time).setAttribute('selected', 'selected');\n        document.getElementById(search_type).setAttribute('selected', 'selected');\n        this.search_content_ = encodeURIComponent(this.s_searchdto.search_content);\n      });\n    },\n    s_page_prev_false: function () {\n      alert('첫 페이지 입니다.');\n    },\n    s_page_next_false: function () {\n      alert('마지막 페이지 입니다.');\n    },\n    service_allservice: function () {\n      location.href = '/service/allService';\n    },\n    service_question_member: function () {\n      location.href = '/service/question-member';\n    },\n    service_question_public: function () {\n      location.href = '/service/question-public';\n    },\n    service_question_guide: function () {\n      location.href = '/service/question-guide';\n    },\n    service_question_write: function () {\n      location.href = '/service/question-write';\n    },\n    service_question_member_search(page_StartBno, search_time, search_type, search_content_) {\n      var url = '/service/question-member?page_NowBno=' + page_StartBno + '&search_time=' + search_time + '&search_type=' + search_type + '&search_content=' + search_content_;\n      location.href = url;\n    },\n    // 글 상세 페이지\n    blist_title_click: function (e) {\n      var val = e.target.parentElement.firstElementChild.value;\n      this.$refs.e_bno_val.value = val;\n      var url = '/service/question-detail?bno=' + val;\n      location.href = url;\n    },\n    // 검색 결과 - 엔터\n    s_content_input_keydown: function (e) {\n      if (e.keyCode == 13) {\n        e.preventDefault();\n        this.$refs.s_content_btn.click();\n      }\n    },\n    // 검색 결과 전달\n    s_content_btn_click: function () {\n      // 검색 기간 등 정보\n      let e_search_time = this.$refs.e_search_time_sel.value;\n      let e_search_type = this.$refs.e_search_type_sel.value;\n      let e_search_content = this.$refs.s_content_input.value;\n      let url = \"/service/question-member\";\n      url += \"?search_time=\" + e_search_time;\n      url += \"&search_type=\" + e_search_type;\n      url += \"&search_content=\" + e_search_content;\n      location.href = url;\n    }\n  }\n};","map":{"version":3,"mappings":";AA+KA,OAAOA,MAAK,MAAO,gCAAgC;AACnD,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,UAAU,EAAE;IACbF;EACC,CAAC;EACDG,IAAG,GAAK;IACR,OAAO;MACPC,MAAK,EAAI,IAAI;MACXC,UAAS,EAAI,IAAI;MACjBC,WAAU,EAAK,IAAI;MACnBC,eAAc,EAAI,IAAI;MACtBC,IAAG,EAAIC,IAAI,CAACC,KAAK,CAACC,cAAc,CAACC,OAAO,CAAC,MAAM,CAAC,CAAC;MACjDC,SAAQ,EAAI;IACd;EACD,CAAC;EACAC,OAAO,GAAG;IACV,IAAI,CAACC,QAAQ,EAAE;EAChB,CAAC;EACCC,OAAO,EAAE;IACXD,QAAQ,GAAE;MACT,IAAI,CAACE,MAAM,CAACC,GAAG,CAAC,0BAA0B,EACzCC,IAAI,CAAEC,GAAG,IAAG;QACZ,IAAI,CAAChB,MAAK,GAAIK,IAAI,CAACC,KAAK,CAACD,IAAI,CAACY,SAAS,CAACD,GAAG,CAACjB,IAAI,CAACC,MAAM,CAAC,CAAC;QACzD,IAAI,CAACC,UAAS,GAAII,IAAI,CAACC,KAAK,CAACD,IAAI,CAACY,SAAS,CAACD,GAAG,CAACjB,IAAI,CAACE,UAAU,CAAC,CAAC;QACjE,IAAI,CAACC,WAAU,GAAIG,IAAI,CAACC,KAAK,CAACD,IAAI,CAACY,