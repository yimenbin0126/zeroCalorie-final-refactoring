ment_List","s_searchdto","s_page","s_dto_list","user","JSON","parse","sessionStorage","getItem","components","mounted","$axios","get","then","res","i","page_EndBno","page_StartBno","page_NowBno","$refs","e_paging_num","innerHTML","standard","search_time","search_type","document","getElementById","setAttribute","methods","service_allservice","location","href","service_question_member","service_question_public","service_question_detail","group_origin","url","service_question_public_myboard","service_question_public_write","service_question_public_search_page_prev","search_content_","service_question_public_search_page_next","blist_title_click","e","val","target","parentElement","firstElementChild","value","e_bno_val","s_content_input_keydown","keyCode","preventDefault","s_content_btn","click","s_content_btn_click","e_search_time","e_search_time_sel","e_search_type","e_search_type_sel","e_search_content","s_content_input","e_con_choice_click","origin_type","result_type","e_con_choice","options","selectedIndex"],"sourceRoot":"","sources":["C:\\Users\\82106\\Desktop\\all\\frontend\\src\\views\\service\\question_public_search.vue"],"sourcesContent":["<template>\n  <div id=\"bodystyle\">\n    <Header />\n    <section>\n\t\t<div id=\"j_wrap\">\n\t\t\t<div id=\"j_box\">\n\n\t\t\t\t<div class=\"e_div\">\n\t\t\t\t\t<!-- 왼쪽 카데고리 -->\n\t\t\t\t\t<nav class=\"e_nav\">\n\t\t\t\t\t\t<!-- 문의 전체보기 -->\n\t\t\t\t\t\t<div class=\"e_nav_all\" @click=\"service_allservice\">\n\t\t\t\t\t\t\t문의 전체보기\n\t\t\t\t\t\t</div>\n\t\t\t\t\t\t<!-- 자주하는 질문 -->\n\t\t\t\t\t\t<div class=\"e_nav_question\" @click=\"service_question_member\">\n\t\t\t\t\t\t\t자주하는 질문\n\t\t\t\t\t\t</div>\n\t\t\t\t\t\t<!-- 공개 건의함 -->\n\t\t\t\t\t\t<div class=\"e_nav_onebyone\" @click=\"service_question_public\">\n\t\t\t\t\t\t\t<div class=\"e_que_div\">공개 건의함</div>\n\t\t\t\t\t\t\t<div><img src=\"@/assets/img/service/category_click.png\"></div>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</nav>\n\n\n\t\t\t\t\t<!-- 오른쪽 내용 -->\n\t\t\t\t\t<div class=\"e_right\">\n\t\t\t\t\t\t<!-- 상단 -->\n\t\t\t\t\t\t<div class=\"e_hd_top\">고객센터 &gt; 공개 건의함</div>\n\t\t\t\t\t\t<div class=\"e_header\">\n\t\t\t\t\t\t\t<div class=\"e_hd_top_que\">공개 건의함</div>\n\t\t\t\t\t\t\t<div class=\"e_hd_top_con\">\n\t\t\t\t\t\t\t\t<span> 공개건의함은 관리자, 회원 등<br>모든 회원이 로그인 한 상태라면<br>\n\t\t\t\t\t\t\t\t\t건의 관련 글을 쓰고 댓글을 남길수 있는 소통 공간입니다.\n\t\t\t\t\t\t\t\t</span>\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\n\t\t\t\t\t\t</div>\n\n\t\t\t\t\t\t<!-- 게시물 불러오기 - 회원 정보 관리 -->\n\t\t\t\t\t\t<div class=\"e_content\">\n\t\t\t\t\t\t\t<!-- 게시물 타입 -->\n\t\t\t\t\t\t\t<div class=\"e_content_choice\">\n\t\t\t\t\t\t\t\t<select id=\"e_con_choice\" ref=\"e_con_choice\"\n\t\t\t\t\t\t\t\t@click=\"e_con_choice_click\">\n\t\t\t\t\t\t\t\t\t<option value=\"new\" id=\"new\">최신순</option>\n\t\t\t\t\t\t\t\t\t<option value=\"view\" id=\"view\">조회수순</option>\n\t\t\t\t\t\t\t\t\t<option value=\"like\" id=\"like\">좋아요순</option>\n\t\t\t\t\t\t\t\t</select>\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t<!-- 게시물 번호 보내기 - 상세보기 -->\n\t\t\t\t\t\t\t<form name=\"e_bno_val_form\" id=\"e_bno_val_form\">\n\t\t\t\t\t\t\t\t<input type=\"hidden\" name=\"bno\" id=\"e_bno_val\">\n\t\t\t\t\t\t\t</form>\n\t\t\t\t\t\t\t<!-- 게시물 목록 시작 -->\n\t\t\t\t\t\t\t<div class=\"e_con_mem\">\n\t\t\t\t\t\t\t\t<ul>\n\t\t\t\t\t\t\t\t\t<li>번호</li>\n\t\t\t\t\t\t\t\t\t<li>제목</li>\n\t\t\t\t\t\t\t\t\t<li>글쓴이</li>\n\t\t\t\t\t\t\t\t\t<li>작성시간</li>\n\t\t\t\t\t\t\t\t\t<li>조회수</li>\n\t\t\t\t\t\t\t\t\t<li>좋아요</li>\n\t\t\t\t\t\t\t\t</ul>\n\t\t\t\t\t\t\t\t\n\t\t\t\t\t\t\t\t<!-- 글 유무 -->\n\t\t\t\t\t\t\t\t<template v-if=\"s_dto_list != null\">\n\t\t\t\t\t\t\t\t\t<ul class=\"e_boardlist\" v-for=\"(s_dto,i) in s_dto_list\" :key=\"i\">\n\t\t\t\t\t\t\t\t\t\t<li :value=\"s_dto.bno\">{{s_dto.bno}}</li>\n\t\t\t\t\t\t\t\t\t\t<li><div v-if=\"s_dto.admin_type == 'reply' && s_page.standard == 'new'\"\n\t\t\t\t\t\t\t\t\t\t\t style=\"font-weight:bold;\">└[답글] </div>\n\t\t\t\t\t\t\t\t\t\t\t<div v-if=\"s_dto.admin_type == 'reply' && s_page.standard != 'new'\"\n\t\t\t\t\t\t\t\t\t\t\t@click=\"service_question_detail(s_dto.group_origin)\">[{{s_dto.group_origin}} 번의 답글]\n\t\t\t\t\t\t\t\t\t\t\t</div><div @click=\"blist_title_click\" class=\"blist_title\">{{s_dto.title}}</div>\n\t\t\t\t\t\t\t\t\t\t\t<div class=\"blist_comment\" \n\t\t\t\t\t\t\t\t\t\t\tv-if=\"comment_List[i] == null\" style=\"color:#1649B0;\"> {{comment_List[i]}}</div>\n\t\t\t\t\t\t\t\t\t\t\t<div class=\"blist_comment\" \n\t\t\t\t\t\t\t\t\t\t\tv-if=\"comment_List[i] != null\" style=\"color:#1649B0;\"> {{comment_List[i]}}</div>\n\t\t\t\t\t\t\t\t\t\t</li>\n\t\t\t\t\t\t\t\t\t\t<li>{{s_dto.nickname}}</li>\n\t\t\t\t\t\t\t\t\t\t<li>{{s_dto.create_time}}</li>\n\t\t\t\t\t\t\t\t\t\t<li>{{s_dto.view_no}}</li>\n\t\t\t\t\t\t\t\t\t\t<li>{{s_dto.like_check}}</li>\n\t\t\t\t\t\t\t\t\t</ul>\n\t\t\t\t\t\t\t\t</template>\n\t\t\t\t\t\t\t\t\n\t\t\t\t\t\t\t\t<template v-if=\"s_dto_list == null\">\n\t\t\t\t\t\t\t\t\t<ul class=\"e_boardlist\">\n\t\t\t\t\t\t\t\t\t\t<li style=\"width:100%; text-align:center;\n\t\t\t\t\t\t\t\t\t\tfont-weight:bold; font-size: 13px;\n\t\t\t\t\t\t\t\t\t\tmargin-top:10px; margin-bottom:10px;\">존재하는 게시물 없음</li>\n\t\t\t\t\t\t\t\t\t</ul>\n\t\t\t\t\t\t\t\t</template>\n\t\t\t\t\t\t\t\t\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t<!-- 게시물 목록 끝 -->\n\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\n\t\t\t\t\t\t<div class=\"e_btn_list\">\n\t\t\t\t\t\t\t<!-- 글쓰기 버튼 보이기 (로그인) -->\n\t\t\t\t\t\t\t<template v-if=\"user != null\">\n\t\t\t\t\t\t\t\t<div>\n\t\t\t\t\t\t\t\t\t<form name=\"myboard_form\">\n\t\t\t\t\t\t\t\t\t\t<input type=\"button\"  value=\"내가 쓴 글 보기\" class=\"e_hd_top_write_myboard\"\n\t\t\t\t\t\t\t\t\t\t@click=\"service_question_public_myboard\">\t\t\t\t\t\t\t\t\n\t\t\t\t\t\t\t\t\t</form>\n\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t<div>\n\t\t\t\t\t\t\t\t\t<input type=\"button\"  value=\"글쓰기\" class=\"e_hd_top_write\"\n\t\t\t\t\t\t\t\t\t@click=\"service_question_public_write\">\n\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t</template>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\n\t\t\t\t\t\t<!-- 페이징 시작 -->\n\t\t\t\t\t\t<div class=\"e_paging\">\n\t\t\t\t\t\t\t<div v-if=\"s_page.page_prev == true\"\n\t\t\t\t\t\t\t@click=\"service_question_public_search_page_prev(s_page.page_StartBno-5, s_searchdto.search_time, s_searchdto.search_type, search_content_)\"\n\t\t\t\t\t\t\t class=\"e_paging_btnleft\" id=\"e_paging_btnleft_yes\">&lt;</div>\n\t\t\t\t\t\t\t\n\t\t\t\t\t\t\t<div v-if=\"s_page.page_prev == false\"\n\t\t\t\t\t\t\t@click=\"alert('첫 페이지 입니다.');\"\n\t\t\t\t\t\t\t class=\"e_paging_btnleft\" id=\"e_paging_btnleft_no\">&lt;</div>\n\t\t\t\t\t\t\t\n\t\t\t\t\t\t\t<div class=\"e_paging_num\">\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\n\t\t\t\t\t\t\t<div v-if=\"s_page.page_next == true\"\n\t\t\t\t\t\t\t@click=\"service_question_public_search_page_next(s_page.page_EndBno+1, s_searchdto.search_time, s_searchdto.search_type, search_content_)\"\n\t\t\t\t\t\t\tclass=\"e_paging_btnright\" id=\"e_paging_btnright_yes\">&gt;</div>\n\t\t\t\t\t\t\t\n\t\t\t\t\t\t\t<div v-if=\"s_page.page_next == false\"\n\t\t\t\t\t\t\t@click=\"alert('마지막 페이지 입니다.');\"\n\t\t\t\t\t\t\t class=\"e_paging_btnright\" id=\"e_paging_btnright_no\">&gt;</div>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t\t<!-- 페이징 끝 -->\n\t\t\t\t\t\t\n\t\t\t\t\t\t<!-- 검색 시작 -->\n\t\t\t\t\t\t<div class=\"e_search\">\n\t\t\t\t\t\t\t<!-- 기간 -->\n\t\t\t\t\t\t\t<div class=\"e_search_time\">\n\t\t\t\t\t\t\t\t<select name=\"e_search_time_sel\" id=\"e_search_time_sel\" ref=\"e_search_time_sel\">\n\t\t\t\t\t\t\t\t\t<option value=\"all\" id=\"all\">전체 기간</option>\n\t\t\t\t\t\t\t\t\t<option value=\"one_day\" id=\"one_day\">1일</option>\n\t\t\t\t\t\t\t\t\t<option value=\"one_week\" id=\"one_week\">1주일</option>\n\t\t\t\t\t\t\t\t\t<option value=\"one_month\" id=\"one_month\">1개월</option>\n\t\t\t\t\t\t\t\t</select>\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t<!-- 타입 -->\n\t\t\t\t\t\t\t<div class=\"e_search_type\">\n\t\t\t\t\t\t\t\t<select name=\"e_search_type_sel\" id=\"e_search_type_sel\" ref=\"e_search_type_sel\">\n\t\t\t\t\t\t\t\t\t<option value=\"title_content\" id=\"title_content\">제목 + 내용</option>\n\t\t\t\t\t\t\t\t\t<option value=\"title\" id=\"title\">제목</option>\n\t\t\t\t\t\t\t\t\t<option value=\"writer\" id=\"writer\">글 작성자</option>\n\t\t\t\t\t\t\t\t</select>\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t<!-- 내용 -->\n\t\t\t\t\t\t\t<div class=\"e_search_content\">\n\t\t\t\t\t\t\t\t<!-- 검색 내용 -->\n\t\t\t\t\t\t\t\t<div class=\"s_content\">\n\t\t\t\t\t\t\t\t\t<input type=\"text\" id=\"s_content_input\" ref=\"s_content_input\"\n\t\t\t\t\t\t\t\t\t@keydown=\"s_content_input_keydown\"\n\t\t\t\t\t\t\t\t\tplaceholder=\"검색어를 입력하세요.\" @focus=\"this.placeholder=''\"\n\t\t\t\t\t\t\t\t\t@blur=\"this.placeholder='검색어를 입력하세요.'\" :value=\"s_searchdto.search_content\">\n\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t<!-- 검색 버튼 -->\n\t\t\t\t\t\t\t\t<button type=\"button\" id=\"s_content_btn\" ref=\"s_content_btn\"\n\t\t\t\t\t\t\t\t@click=\"s_content_btn_click\">\n\t\t\t\t\t\t\t\t\t검색\n\t\t\t\t\t\t\t\t</button>\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t\t<!-- 검색 끝 -->\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\n\t\t\t</div>\n\t\t</div>\n\t</section>\n  </div>\n</template>\n\n<script>\nimport Header from \"@/components/common/Header.vue\";\nexport default {\n  name: \"question_public_search\",\n  data : function() {\n    return {\n        comment_List : null,\n        s_searchdto : null,\n\t\ts_page : null,\n\t\ts_dto_list : null,\n\t\tuser : JSON.parse(sessionStorage.getItem('user')),\n    }\n  },\n  components: {\n\tHeader,\n  },\n  mounted() {\n\tthis.$axios.get('/service/question-public-search')\n\t\t.then((res)=>{\n\t\t\tthis.comment_List = res.data.comment_List;\n\t\t\tthis.s_searchdto = res.data.s_searchdto;\n\t\t\tthis.s_page = res.data.s_page;\n\t\t\tthis.s_dto_list = res.data.s_dto_list;\n\t\t\t// 페이지 값\n\t\t\tfor(var i=1; i<this.s_page.page_EndBno-this.s_page.page_StartBno+1; i++){\n\t\t\t\tif (i==this.s_page.page_NowBno){\n\t\t\t\t\tthis.$refs.e_paging_num.innerHTML += '<a id=\"page_NowBno\">'+i+'</a>';\n\t\t\t\t} else {\n\t\t\t\t\tthis.$refs.e_paging_num.innerHTML += '<a '+\n\t\t\t\t\t'href=\"'+\"/service/question-public-search?page_NowBno=\"+i+\n\t\t\t\t\t// eslint-disable-next-line\n\t\t\t\t\t'&standard='+s_page.standard+'\"'\n\t\t\t\t\t+'class=\"page_Bno\" id=\"page_Bno'+i+'\">'+i+'</a>';\n\t\t\t\t}\n\t\t\t}\n\t\t\tvar standard = this.s_page.standard;\n\t\t\tvar search_time = this.s_searchdto.search_time;\n\t\t\tvar search_type = this.s_searchdto.search_type;\n\t\t\tdocument.getElementById(search_time).setAttribute('selected','selected');\n\t\t\tdocument.getElementById(search_type).setAttribute('selected','selected');\n\t\t\tdocument.getElementById(standard).setAttribute('selected','selected');\n\t\t})\n\t},\n  methods: {\n\t\tservice_allservice: function(){\n            location.href='/service/allService'\n        },\n        service_question_member: function(){\n            location.href='/service/question-member'\n        },\n        service_question_public: function(){\n            location.href='/service/question-public'\n        },\n        service_question_detail(group_origin){\n\t\t\tvar url = '/service/question-public-detail?bno='+group_origin;\n            location.href= url\n        },\n\t\tservice_question_public_myboard: function(){\n            location.href='/service/question-public-myboard'\n        },\n\t\tservice_question_public_write: function(){\n            location.href='/service/question-public-write'\n        },\n\t\tservice_question_public_search_page_prev(page_StartBno, search_time, search_type, search_content_){\n\t\t\tvar url = '/service/question-public-search?page_NowBno='+page_StartBno+'&search_time='+search_time+'&search_type='+search_type+'&search_content='+search_content_;\n            location.href=url\n        },\n\t\tservice_question_public_search_page_next(page_StartBno, search_time, search_type, search_content_){\n            var url = '/service/question-public-search?page_NowBno='+page_StartBno+'&search_time='+search_time+'&search_type='+search_type+'&search_content='+search_content_;\n            location.href=url\n        },\n\t\t// 글 상세 페이지\n\t\tblist_title_click:function (e){\n\t\t\tvar val = e.target.parentElement.parentElement.firstElementChild.value;\n\t\t\tthis.$refs.e_bno_val.value = val;\n\t\t\tvar url = '/service/question-public-detail?bno='+val;\n\t\t\tlocation.href= url;\n\t\t},\n\t\t// 검색 결과 - 엔터\n\t\ts_content_input_keydown:function (e){\n\t\t\tif(e.keyCode ==13){\n\t\t\t\te.preventDefault();\n\t\t\t\tthis.$refs.s_content_btn.click();\n\t\t\t}\n\t\t},\n\t\t// 검색 결과 전달\n\t\ts_content_btn_click:function (){\n\t\t\t// 검색 기간 등 정보\n\t\t\tlet e_search_time = this.$refs.e_search_time_sel.value;\n\t\t\tlet e_search_type = this.$refs.e_search_type_sel.value;\n\t\t\tlet e_search_content = this.$refs.s_content_input.value;\n\t\t\tlet url = \"/service/question-public-search\";\n\t\t\turl += \"?search_time=\"+e_search_time;\n\t\t\turl += \"&search_type=\"+e_search_type;\n\t\t\turl += \"&search_content=\"+e_search_content;\n\t\t\tlocation.href= url;\n\t\t},\n\t\te_con_choice_click:function (){\n\t\t\tvar origin_type = this.s_page.standard;\n\t\t\tvar result_type = this.$refs.e_con_choice.options[this.$refs.e_con_choice.selectedIndex].value;\n\t\t\t// 값이 다르면 실행\n\t\t\tif (origin_type != result_type){\n\t\t\t\tvar url = \"/service/question-public?\";\n\t\t\t\turl += \"standard=\"+ result_type;\n\t\t\t\tlocation.href=url;\n\t\t\t}\n\t\t}\n\t}\n}\n</script>\n\n<style lang=\"scss\" scoped>\n\t@import \"@/assets/scss/service/question_public_search.scss\";\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}