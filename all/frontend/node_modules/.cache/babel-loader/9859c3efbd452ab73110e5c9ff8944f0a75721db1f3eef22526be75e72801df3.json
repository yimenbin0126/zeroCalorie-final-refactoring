pe=\"password\" name=\"pw\" id=\"e_input_pass\" ref=\"e_input_pass\"\n                  placeholder=\"비밀번호를 입력해 주세요.\" @focus=\"this.placeholder=''\"\n                  @blur=\"this.placeholder='비밀번호를 입력해 주세요.'\">\n              </div>\n    \n              <!-- 로그인 버튼-->\n              <div class=\"e_login\">\n                <input type=\"submit\" id=\"e_login_btn\" value=\"로그인\"\n                @click=\"login_sub\" @keydown=\"login_keydown\">\n              </div>\n    \n              <!-- 회원가입, 비밀번호 찾기 -->\n              <div class=\"e_other\">\n                <a class=\"e_join_member\" href=\"/member/join\">회원가입</a> <a\n                  class=\"e_find_pass\" href=\"/member/findid\">아이디/비밀번호 찾기</a>\n              </div>\n            </form>\n          </div>\n        </div>\n      </section>\n  </div>\n</template>\n\n<script>\nimport Header from \"@/components/common/Header.vue\";\nexport default {\n  name: \"login\",\n  components: {\n    Header,\n  },\n  mounted() {\n\t  this.$axios.get('/member/login')\n      .then(()=>{\n        if(sessionStorage.getItem('user') == null){\n          location.href=\"/\";\n        }\n      })\n\t},\n  methods: {\n    login_sub: function(e){\n      e.preventDefault();\n      if (this.$refs.e_input_id.value.length == 0) {\n         alert(\"아이디를 입력해주세요.\");\n        } else {\n          if (this.$refs.e_input_pass.value.length == 0) {\n            alert(\"비밀번호를 입력해주세요.\");\n         } else {\n            this.$axios.post('/member/login', null, {\n              params: {\n              id: this.$refs.e_input_id.value,\n              pw: this.$refs.e_input_pass.value,\n            }})\n            .then(res=>{\n              if (res.data == null) {\n                  alert('아이디 혹은 비밀번호가 틀렸습니다.');\n               } else {\n                  sessionStorage.setItem('user', JSON.stringify(res.data));\n                  var userr = JSON.parse(sessionStorage.getItem('user'));\n                  console.log(userr);\n                  console.log(userr.nickname);\n                  //location.href = \"/\";\n               }\n            })\n         }\n      }\n    },\n    login_keydown:function (e){\n\t\t  if(e.keyCode ==13){\n        \te.preventDefault();\n          this.login_sub();\n      }\n\t  },\n  }\n};\n</script>\n\n<style lang=\"scss\" scoped>\n  @import \"@/assets/scss/member/login.scss\";\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}