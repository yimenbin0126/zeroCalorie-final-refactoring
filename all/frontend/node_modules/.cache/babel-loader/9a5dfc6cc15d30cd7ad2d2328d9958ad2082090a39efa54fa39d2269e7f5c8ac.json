{"ast":null,"code":"import Header from \"@/components/common/Header.vue\";\nexport default {\n  name: \"question_guide_search\",\n  data: function () {\n    return {\n      s_page: null,\n      s_dto_list: null,\n      s_searchdto: null,\n      search_content_: null,\n      user: this.$session.get('user')\n    };\n  },\n  components: {\n    Header\n  },\n  mounted() {\n    this.$axios.get('/service/question-guide-search').then(res => {\n      this.s_page = res.s_page;\n      this.s_dto_list = res.s_dto_list;\n      this.s_searchdto = res.s_searchdto;\n      // 페이지 값\n      for (var i = 1; i < this.s_page.page_EndBno - this.s_page.page_StartBno + 1; i++) {\n        if (i == this.s_page.page_NowBno) {\n          this.$refs.e_paging_num.innerHTML += '<a id=\"page_NowBno\">' + i + '</a>';\n        } else {\n          this.$refs.e_paging_num.innerHTML += '<a ' + 'href=' + '\"/service/question-guide-search?page_NowBno=' + i +\n          // eslint-disable-next-line\n          '&search_time=' + s_searchdto.search_time\n          // eslint-disable-next-line\n          + '&search_type=' + s_searchdto.search_type\n          // eslint-disable-next-line\n          + '&search_content=' + s_searchdto.search_type + '\"' + 'class=\"page_Bno\">' + i + '</a>';\n        }\n      }\n      // 검색 값\n      // eslint-disable-next-line\n      var search_time = this.s_searchdto.search_time;\n      var search_type = this.s_searchdto.search_type;\n      document.getElementById(search_time).setAttribute('selected', 'selected');\n      document.getElementById(search_type).setAttribute('selected', 'selected');\n      // eslint-disable-next-line\n      search_content_ = URLEncoder.encode(this.s_searchdto.search_content, \"UTF-8\");\n    });\n  },\n  methods: {\n    service_allservice: function () {\n      location.href = '/service/allService';\n    },\n    service_question_member: function () {\n      location.href = '/service/question-member';\n    },\n    service_question_guide: function () {\n      location.href = '/service/question-guide';\n    },\n    service_question_write: function () {\n      location.href = '/service/question-write';\n    },\n    service_question_public: function () {\n      location.href = '/service/question-public';\n    },\n    service_question_guide_search(page_NowBno, search_time, search_type, search_content) {\n      var url = '/service/question-guide-search?page_NowBno=' + page_NowBno + '&search_time=' + search_time + '}&search_type=' + search_type + '&search_content=' + search_content;\n      location.href = url;\n    },\n    // 글 상세 페이지\n    blist_title_click: function (e) {\n      var val = e.target.parentElement.firstElementChild.value;\n      this.$refs.e_bno_val.value = val;\n      var url = '/service/question-detail?bno=' + val;\n      location.href = url;\n    },\n    // 검색 결과 - 엔터\n    s_content_input_keydown: function (e) {\n      if (e.keyCode == 13) {\n        e.preventDefault();\n        this.$refs.s_content_btn.click();\n      }\n    },\n    // 검색 결과 전달\n    s_content_btn_click: function () {\n      // 검색 기간 등 정보\n      let e_search_time = this.$refs.e_search_time_sel.value;\n      let e_search_type = this.$refs.e_search_type_sel.value;\n      let e_search_content = this.$refs.s_content_input.value;\n      let url = \"/service/question-guide-search\";\n      url += \"?search_time=\" + e_search_time;\n      url += \"&search_type=\" + e_search_type;\n      url += \"&search_content=\" + e_search_content;\n      location.href = url;\n    }\n  }\n};","map":{"version":3,"mappings":"AAwKA,OAAOA,MAAK,MAAO,gCAAgC;AACnD,eAAe;EACbC,IAAI,EAAE,uBAAuB;EAC7BC,IAAG,EAAI,YAAW;IAChB,OAAO;MACHC,MAAK,EAAI,IAAI;MACnBC,UAAS,EAAI,IAAI;MACjBC,WAAU,EAAK,IAAI;MACnBC,eAAc,EAAI,IAAI;MACtBC,IAAG,EAAI,IAAI,CAACC,QAAQ,CAACC,GAAG,CAAC,MAAM;IAC7B;EACF,CAAC;EACDC,UAAU,EAAE;IACbV;EACC,CAAC;EACDW,OAAO,GAAG;IACX,IAAI,CAACC,MAAM,CAACH,GAAG,CAAC,gCAAgC,EAC1CI,IAAI,CAAEC,GAAG,IAAG;MACX,IAAI,CAACX,MAAK,GAAIW,GAAG,CAACX,MAAM;MACxB,IAAI,CAACC,UAAS,GAAIU,G