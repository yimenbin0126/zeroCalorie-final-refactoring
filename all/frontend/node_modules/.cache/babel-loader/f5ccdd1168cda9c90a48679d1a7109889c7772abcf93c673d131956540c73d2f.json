{"ast":null,"code":"import Header from \"@/components/common/Header.vue\";\nexport default {\n  name: \"App\",\n  data: function () {\n    return {\n      s_page: null,\n      s_dto_list: null,\n      s_searchdto: null,\n      search_content_: null,\n      user: this.$session.get('user')\n    };\n  },\n  components: {\n    Header\n  },\n  mounted() {\n    this.$axios.get('/service/question-member-search').then(res => {\n      this.s_page = res.s_page;\n      this.s_dto_list = res.s_dto_list;\n      this.s_searchdto = res.s_searchdto;\n      // 페이지 값\n      for (var i = 1; i < this.s_page.page_EndBno - this.s_page.page_StartBno + 1; i++) {\n        if (i == this.s_page.page_NowBno) {\n          this.$refs.e_paging_num.innerHTML += '<a id=\"page_NowBno\">' + i + '</a>';\n        } else {\n          this.$refs.e_paging_num.innerHTML += '<a ' + 'href=' + '\"/service/question-member-search?page_NowBno=' + i + '&search_time=' + s_searchdto.search_time + '&search_type=' + s_searchdto.search_type + '&search_content=' + s_searchdto.search_type + '\"' + 'class=\"page_Bno\">' + i + '</a>';\n        }\n      }\n      // 검색 값\n      var search_time = this.s_searchdto.search_time;\n      var search_type = this.s_searchdto.search_type;\n      document.getElementById(search_time).setAttribute('selected', 'selected');\n      document.getElementById(search_type).setAttribute('selected', 'selected');\n      search_content_ = URLEncoder.encode(this.s_searchdto.search_content, \"UTF-8\");\n    });\n  },\n  methods: {\n    // 글 상세 페이지\n    blist_title_click: function (e) {\n      var val = e.target.parentElement.firstElementChild.value;\n      this.$refs.e_bno_val.value = val;\n      var url = '/service/question-detail?bno=' + val;\n      location.href = url;\n    },\n    // 검색 결과 - 엔터\n    s_content_input_keydown: function (e) {\n      if (e.keyCode == 13) {\n        e.preventDefault();\n        this.$refs.s_content_btn.click();\n      }\n    },\n    // 검색 결과 전달\n    s_content_btn_click: function () {\n      // 검색 기간 등 정보\n      let e_search_time = this.$refs.e_search_time_sel.value;\n      let e_search_type = this.$refs.e_search_type_sel.value;\n      let e_search_content = this.$refs.s_content_input.value;\n      let url = \"/service/question-member-search\";\n      url += \"?search_time=\" + e_search_time;\n      url += \"&search_type=\" + e_search_type;\n      url += \"&search_content=\" + e_search_content;\n      location.href = url;\n    }\n  }\n};","map":{"version":3,"mappings":"AA0KA,OAAOA,MAAK,MAAO,gCAAgC;AAEnD,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,IAAG,EAAI,YAAW;IAChB,OAAO;MACHC,MAAK,EAAI,IAAI;MACnBC,UAAS,EAAI,IAAI;MACjBC,WAAU,EAAK,IAAI;MACnBC,eAAc,EAAI,IAAI;MACtBC,IAAG,EAAI,IAAI,CAACC,QAAQ,CAACC,GAAG,CAAC,MAAM;IAC7B;EACF,CAAC;EACDC,UAAU,EAAE;IACVV;EACF,CAAC;EACDW,OAAO,GAAG;IACX,IAAI,CAACC,MAAM,CAACH,GAAG,CAAC,iCAAiC,EAC3CI,IAAI,CAAEC,GAAG,IAAG;MACX,IAAI,CAACX,MAAK,GAAIW,GAAG,CAACX,MAAM;MACxB,IAAI,CAACC,UAAS,GAAIU,GAAG,CAACV,UAAU;MACtC,IAAI,CAACC,WAAU,GAAIS,GAAG,CAACT,WAAW;MAClC;MACA,KAAI,IAAIU,CAAC,GAAC,CAAC,EAAEA,CAAC,GAAC,IAAI,CAACZ,MAAM,CAACa,WAAW,GAAC,IAAI,CAACb,MAAM,CAACc,aAAa,GAAC,CAAC,EAAEF,CAAC,EAAE,EAAC;QACvE,IAAIA,CAAC,IAAE,IAAI,CAACZ,MAAM,CAACe,WAAW,EAAC;UAC9B,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,SAAQ,IAAK,sBAAsB,GAACN,CAAC,GAAC,MAAM;QACrE,OAAO;UACN,IAAI,CAACI,KAAK,CAACC,YAAY,CAACC,SAAQ,IAAK,KAAK,GAC1C,OAAO,GAAC,+CAA+C,GAACN,CAAC,GACzD,eAAe,GAACV,WAAW,CAACiB,WAAU,GACrC,eAAe,GAACjB,WAAW,CAACkB,WAAU,GACtC,kBAAkB,GAAClB,WAAW,CAACkB,WAAW,GAC3C,GAAG,GAAC,mBAAmB,GAACR,CAAC,GAAC,MAAM;QACjC;MACD;MACA;MACA,IAAIO,WAAU,GAAI,IAAI,CAACjB,WAAW,CAACiB,WAAW;MAC9C,IAAIC,WAAU,GAAI,IAAI,CAAClB,WAAW,CAACkB,WAAW;MAC9CC,QAAQ,CAACC,cAAc,CAACH,WAAW,CAAC,CAACI,YAAY,CAAC,UAAU,EAAC,UAAU,CAAC;MACxEF,QAAQ,CAACC,cAAc,CAACF,WAAW,CAAC,CAACG,YAAY,CAAC,UAAU,EAAC,UAAU,CAAC;MACxEpB,eAAc,GAAIqB,UAAU,CAACC,MAAM,CAAC,IAAI,CAACvB,WAAW,CAACwB,cAAc,EAAE,OAAO,CAAC;IACzE,CAAC;EACP,CAAC;EACCC,OAAO,EAAE;IACV;IACAC,iBAAiB,EAAC,UAAUC,CAAC,EAAC;MAC7B,IAAIC,GAAE,GAAID,CAAC,CAACE,MAAM,