tion () {\n      location.href = '/service/question-write';\n    },\n    service_question_member_search(page_StartBno, search_time, search_type, search_content_) {\n      this.$axios.get('/service/question-member', {\n        params: {\n          \"page_NowBno\": page_StartBno,\n          \"search_time\": search_time,\n          \"search_type\": search_type,\n          \"search_contentv\": search_content_\n        }\n      }).then(res => {\n        this.goback();\n        this.s_page = JSON.parse(JSON.stringify(res.data.s_page));\n        this.s_dto_list = JSON.parse(JSON.stringify(res.data.s_dto_list));\n        this.s_searchdto = JSON.parse(JSON.stringify(res.data.s_searchdto));\n        for (var i = this.s_page.page_StartBno; i <= this.s_page.page_EndBno; i++) {\n          this.page_list.push(i);\n        }\n        var search_time = this.s_searchdto.search_time;\n        var search_type = this.s_searchdto.search_type;\n        document.getElementById(search_time).selected = true;\n        document.getElementById(search_type).selected = true;\n        document.querySelector('#s_content_input').value = this.s_searchdto.search_content;\n      });\n    },\n    // 글 상세 페이지\n    blist_title_click: function (e) {\n      var val = e.target.parentElement.firstElementChild.value;\n      this.$refs.e_bno_val.value = val;\n      var url = '/service/question-detail?bno=' + val;\n      location.href = url;\n    },\n    // 검색 결과 - 엔터\n    s_content_input_keydown: function (e) {\n      if (e.keyCode == 13) {\n        e.preventDefault();\n        this.$refs.s_content_btn.click();\n      }\n    },\n    // 검색 결과 전달\n    s_content_btn_click: function () {\n      // 검색 기간 등 정보\n      let e_search_time = this.$refs.e_search_time_sel.value;\n      let e_search_type = this.$refs.e_search_type_sel.value;\n      let e_search_content = this.$refs.s_content_input.value;\n      this.$axios.get('/service/question-member', {\n        params: {\n          \"search_time\": e_search_time,\n          \"search_type\": e_search_type,\n          \"search_contentv\": e_search_content\n        }\n      }).then(res => {\n        this.goback();\n        this.s_page = JSON.parse(JSON.stringify(res.data.s_page));\n        this.s_dto_list = JSON.parse(JSON.stringify(res.data.s_dto_list));\n        this.s_searchdto = JSON.parse(JSON.stringify(res.data.s_searchdto));\n        for (var i = this.s_page.page_StartBno; i <= this.s_page.page_EndBno; i++) {\n          this.page_list.push(i);\n        }\n        var search_time = this.s_searchdto.search_time;\n        var search_type = this.s_searchdto.search_type;\n        document.getElementById(search_time).selected = true;\n        document.getElementById(search_type).selected = true;\n        document.querySelector('#s_content_input').value = this.s_searchdto.search_content;\n      });\n    }\n  }\n};","map":{"version":3,"mappings":";AA+KA,OAAOA,MAAK,MAAO,gCAAgC;AACnD,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,UAAU,EAAE;IACbF;EACC,CAAC;EACDG,IAAG,GAAK;IACR,OAAO;MACNC,MAAK,EAAI,IAAI;MACbC,UAAS,EAAI,IAAI;MACjBC,WAAU,EAAK,IAAI;MACnBC,eAAc,EAAI,IAAI;MACtBC,IAAG,EAAIC,IAAI,CAACC,KAAK,CAACC,cAAc,CAACC,OAAO,CAAC,MAAM,CAAC,CAAC;MACjDC,SAAQ,EAAI;IACb;EACD,CAAC;EACAC,OAAO,GAAG;IACV,IAAI,CAACC,QAAQ,EAAE;EAChB,CAAC;EACCC,OAAO,EAAE;IACXD,QAAQ,GAAE;MACT,IAAI,CAACE,MAAM,CAACC,GAAG,CAAC,0BAA0B,EACzCC,IAAI,CAAEC,GAAG,IAAG;QACZ,IAAI,CAAChB,MAAK,GAAIK,IAAI,CAACC,KAAK,CAACD,IAAI,CAACY,SAAS,CAACD,GAAG,CAACjB,IAAI,CAACC,MAAM,CAAC,CAAC;QACzD,IAAI,CAACC,UAAS,GAAII,IAAI,CAACC,KAAK,CAACD,IAAI,CAACY,SAAS,CAACD,GAAG,CAACjB,IAAI,CAACE,UAAU,CAAC,CAAC;QACjE,IAAI,CAACC,WAAU,GAAIG,IAAI,CAACC,KAAK,CAACD,IAAI,CAACY,SAAS,CAACD,GAAG,CAACjB,IAAI,CAACG,WAAW,CAAC,CAAC;QACnE,KAAI,IAAIgB,CAAC,GAAC,IAAI,CAAClB,MAAM,CAACmB,aAAa,EAAED,CAAC,IAAE,IAAI,CAAClB,MAAM,CAACoB,WAAW,EAAEF,CAAC,EAAE,EAAC;UACpE,IAAI,CAACT,SAAS,CAACY,IAAI,CAACH,CAAC,CAAC;QACvB;QACA,IAAII,WAAU,GAAI,IAAI,CAACpB,WAAW,CAACoB,WAAW;QAC9C,IAAIC,WAAU,GAAI,IAAI,CAACrB,WAAW,CAACqB,WAAW;QAC9CC,OAAO,CAACC,GAAG,CAA