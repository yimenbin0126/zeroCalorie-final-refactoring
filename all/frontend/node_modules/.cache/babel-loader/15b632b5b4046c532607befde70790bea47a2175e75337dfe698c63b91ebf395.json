er/findpass-new', {\n\t\t\tpass : this.$axios.e_input_code.value,\n\t\t    pass_re : this.$axios.e_input_code_re.value\n        })\n        .then((res)=>{\n\t\t\tif(res==\"O\"){\n\t\t\t\talert('비밀번호가 변경되었습니다.');\n\t\t\t\tlocation.href='/member/findpass';\n\t\t\t} else if (res==\"X1\") {\n\t\t\t\talert('기존 비밀번호와 똑같은 비밀번호입니다.\\n다시 입력해주세요.');\n\t\t\t} else if (res==\"X2\") {\n\t\t\t\talert('비밀번호가 올바르지 않습니다.\\n다시 입력해주세요.');\n\t\t\t} else if (res==\"X3\") {\n\t\t\t\talert('비밀번호 재입력이 올바르지 않습니다.\\n다시 입력해주세요.');\n\t\t\t}\n        })\n\t}\n  }\n};\n</script>\n\n<style lang=\"scss\">\n\t@import \"@/assets/scss/member/findpass-new.scss\";\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}