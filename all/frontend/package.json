{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --watch",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^1.2.0",
    "axois": "^0.0.1-security",
    "core-js": "^3.8.3",
    "node-sass": "^8.0.0",
    "sass-loader": "^13.2.0",
    "vue": "^3.2.13",
    "vue-router": "^4.0.12",
    "vue-session": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^7.0.0-beta.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
